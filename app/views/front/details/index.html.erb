<div class='row'>
    <div id="map"></div>

    <script>
      function initMap() {
        var mapDiv = document.getElementById('map');
        var map = new google.maps.Map(mapDiv, {
            center: {lat: 44.540, lng: -78.546},
            zoom: 8
        });
      }
    </script>

    <script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAXD9HFiiYIA9-LMRyt4PbiXmzaAtmwrtI&callback=initMap">
    </script>
</div>
