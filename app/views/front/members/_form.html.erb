<%= form_for [:front, @member] do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <%= f.label :profession %>
  <%= f.text_field :profession %>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <%= f.file_field :avatar %>
  <%= f.submit %>
<% end %>
